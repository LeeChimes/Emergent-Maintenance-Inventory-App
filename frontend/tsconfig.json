{
    "compilerOptions":  {
                            "target":  "ES2020",
                            "module":  "ESNext",
                            "lib":  [
                                        "ES2020",
                                        "DOM"
                                    ],
                            "jsx":  "react-jsx",
                            "moduleResolution":  "Bundler",
                            "resolveJsonModule":  true,
                            "strict":  true,
                            "allowJs":  false,
                            "noEmit":  true,
                            "isolatedModules":  true,
                            "skipLibCheck":  true,
                            "esModuleInterop":  true,
                            "types":  [
                                          "node",
                                          "react",
                                          "react-native"
                                      ],
                            "baseUrl":  ".",
                            "paths":  {
                                          "@/*":  [
                                                      "*"
                                                  ]
                                      }
                        },
    "include":  [
                    "**/*.ts",
                    "**/*.tsx",
                    "expo-env.d.ts",
                    ".expo/types/**/*.ts"
                ],
    "extends":  "expo/tsconfig.base"
}